<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MB.MagicCMS.4.5</id>
		<version>$version$</version>
		<authors>Bruno Migliaretti</authors>
		<owners>Bruno Migliaretti</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Content Management System framework for Visual studio</description>
		<releaseNotes>Release for .NET 4.7.x</releaseNotes>
		<copyright>Copyright 2016-2022</copyright>
		<dependencies>
			<dependency id="Facebook" version="7.0.6" />
			<dependency id="jQuery" version="3.7.1"  />
			<dependency id="json2" version="1.0.2" />
			<dependency id="Respond" version="1.4.2" />
			<dependency id="Microsoft.AspNet.WebApi" version="5.2.7" />
			<dependency id="RestSharp" version="110.2.0" />
			<dependency id="MimeTypesMap" version="1.0.8" />		
