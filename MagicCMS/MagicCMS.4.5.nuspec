		<releaseNotes>First release for .NET 4.5</releaseNotes>
		<copyright>Copyright 2016</copyright>
		<dependencies>
			<dependency id="Facebook" version="7.0.6" />
			<dependency id="jQuery" version="2.2.4"  />
			<dependency id="jQuery1" version="1.12.3" />
			<dependency id="json2" version="1.0.2" />
			<dependency id="MB.FileBrowser" version="1.1.8" />
