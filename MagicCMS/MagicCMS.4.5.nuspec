		<file src="Default.aspx" target="content\"/>
		<file src="error.aspx" target="content\"/>
		<file src="Min.ashx" target="content\"/>
		<file src="SiteMaster.Master" target="content\" />
		<file src="Global.asax.pp" target="content\"/>
		<file src="Global.asax.cs.pp" target="content\"/>
		<file src="web.config.install.xdt" target="content\"/>
		<file src="bin\MagicCMS.dll" target="lib\Net45\"/>
		<file src="bin\MagicCMS.pdb" target="lib\Net45\"/>
	</files>
</package>
